    current_item_style: (fg: "blue", bg: "black"),
    borders_style: (fg: "blue"),
    highlight_border_style: (fg: "blue"),
    symbols: (
        song: "Û∞ùö",
        song_style: (fg: "blue"),
        dir: "Û∞âã",
