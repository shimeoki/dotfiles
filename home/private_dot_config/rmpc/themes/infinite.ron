                        ),
                    ]
                )
            ),
            (
                size: "100%",
                pane: Pane(TabContent),
            ),
        ],
    ),
    header: (
        rows: [
            (
                left: [
                    (
                        kind: Property(Song(Artist)),
                        style: (fg: "cyan"),
                        default: (
                            kind: Text("artist?"),
                            style: (fg: "cyan"),
                        ),
                    ),
                ],
                center: [
                    (
                        kind: Property(Song(Title)), 
                        style: (modifiers: "Bold"),
                        default: (
                            kind: Property(Song(Filename)),
                            style: (modifiers: "Bold"),
                            default: (
                                kind: Text("name?"),
                                style: (modifiers: "Bold"),
                            ),
                        ),
                    )
                ],
                right: [
                    (
                        kind: Property(Song(Album)),
                        style: (fg: "magenta"),
                        default: (
                            kind: Text("album?"),
                            style: (fg: "magenta"),
                        )
                    )
                ]
            ),
            (
                left: [
                    (kind: Property(Status(StateV2(
                        playing_label: "Û∞êä",
                        playing_style: (fg: "green"),
