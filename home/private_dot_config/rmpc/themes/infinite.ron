                    (kind: Text(" ")),
                    (kind: Property(Status(Elapsed))),
                    (kind: Text(" / ")),
                    (kind: Property(Status(Duration))),
                    (kind: Text(" at ")),
                    (kind: Property(Status(Volume)), style: (fg: "blue")),
                    (kind: Text("%"), style: (fg: "blue")),
                ],
                center: [
                    (kind: Property(Status(RepeatV2(
                        on_label: "repeat",
                        on_style: (fg: "green"),
                        off_label: "repeat",
                        off_style: (fg: "dark_gray"),
                    )))),
                    (kind: Text(" | ")),
                    (kind: Property(Status(RandomV2(
                        on_label: "random",
                        on_style: (fg: "green"),
                        off_label: "random",
                        off_style: (fg: "dark_gray"),
                    )))),
                ],
                right: [
                    (kind: Property(Status(ConsumeV2(
                        on_label: "remove",
                        on_style: (fg: "green"),
                        oneshot_label: "remove",
                        oneshot_style: (fg: "yellow"),
                        off_label: "remove",
                        off_style: (fg: "dark_gray"),
                    )))),
                    (kind: Text(" | ")),
                    (kind: Property(Status(SingleV2(
                        on_label: "single",
                        on_style: (fg: "green"),
                        oneshot_label: "single",
                        oneshot_style: (fg: "yellow"),
                        off_label: "single",
                        off_style: (fg: "dark_gray"),
                    )))),
                ]
            ),
        ],
    ),
    browser_song_format: [
        (
            kind: Group([
                (kind: Property(Track)),
                (kind: Text(" ")),
                (kind: Property(Title)),
                (kind: Text(" - ")),
                (kind: Property(Duration)),
            ]),
            default: (
                kind: Group([
                    (kind: Property(Filename)),
                    (kind: Text(" - ")),
                    (kind: Property(Duration)),
                ])
            ),
        )
    ],
    lyrics: (timestamp: false),
    cava: (
        inverted_bar_symbols: ['‚ñî', 'üÆÇ', 'üÆÉ', '‚ñÄ', 'üÆÑ', 'üÆÖ', 'üÆÜ', '‚ñà'],
        bar_symbols: ['‚ñÅ', '‚ñÇ', '‚ñÉ', '‚ñÑ', '‚ñÖ', '‚ñÜ', '‚ñá', '‚ñà'],
        bar_width: 2,
